---
  root: true
  extends: airbnb-base
  env:
    browser: false
    node: true
    mocha: true
    es6: true
  rules:
    comma-dangle:
      - 2
      - "never"
    no-console: 0
    no-param-reassign: 0
    space-before-function-paren: 0
    max-len: 0
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/test/**"]}]
